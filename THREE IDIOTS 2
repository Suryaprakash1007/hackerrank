import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int x1=sc.nextInt();
        int y1=sc.nextInt();
        int x2=sc.nextInt();
        int y2=sc.nextInt();
        float m1=(x1+x2)/2.0f;
        float m2=(y1+y2)/2.0f;
        System.out.println("Binoy's house is located at ("+m1+","+m2+")");
        sc.close();
    }
        
}
